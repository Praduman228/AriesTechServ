/* contact us  */
/* General Styles */
/* General Styles */
.contact {
    width: 100%;
    background-color: white;
    padding: 5% 10%;
}

.contain {
    background-color: white;
    max-width: 1170px;
    width: 100%;
    margin: auto;
}

.wrapper {
    display: flex;
    flex-direction: column;
}

.form {
    background-color: white;
}

.contact-wrapper {
    margin-top: 20px;
}

/* Form Styling */
.form span {
text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.2);
font-size: 30px;
color: #0694C3;
font-size: 30px;
}

.submit-btn, .reset-btn {
    width: 100%;
    text-align: center;
}

.form-headline:after {
    content: "";
    display: block;
    width: 50px;
    border-bottom: 3px solid #0694C3;
    margin-top: 10px;
}

.form-input, textarea {
    width: 100%;
    border: 1px solid #bdbdbd;
    border-radius: 5px;
    padding: 10px;
}

textarea {
    resize: none;
}

button, .submit-btn {
    background: transparent;
    border: 1px solid #0694C3;
    color: #0694C3;
    border-radius: 15px;
    padding: 10px;
    width: 100%;
    cursor: pointer;
    text-transform: uppercase;
}

button:hover, .submit-btn:hover {
    background: #0694C3;
    color: white;
}

.error {
    color: #ec1c24;
}

/* Contact Details */
.contacts {
    color: #212d31;
    text-align: center;
    padding: 20px;
}

.hightlight-contact-info {
    font-weight: 700;
    font-size: 18px;
    line-height: 1.5;
}

.email-info {
    margin-top: 15px;
}




/* Responsive Styles */
@media (min-width: 700px) {
    .contact{
        width: 79%;
    }
    .wrapper {
        display: grid;
        grid-template-columns: 2fr 1fr;
        gap: 20px;
    }
    .submit-btn, .reset-btn {
        width: auto;
    }
    .footer {
        flex-direction: row;
        justify-content: space-around;
        text-align: left;
    }
    .footer1, .footer2, .footer3 {
        max-width: 250px;
        text-align: left;
    }
}

